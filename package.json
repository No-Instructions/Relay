{
	"name": "relay",
	"version": "1.0.0",
	"description": "Real-time collaborative workspace for Obsidian.",
	"main": "main.js",
	"type": "module",
	"scripts": {
		"dev": "node esbuild.config.mjs watch",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"beta": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs debug",
		"staging": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs staging",
		"version": "node version-bump.mjs && git add manifest.json versions.json",
		"debugger": "NODE_OPTIONS=--experimental-specifier-resolution=node ts-node --esm crdt.ts",
		"test": "jest",
		"lint": "npx eslint . --ext .ts"
	},
	"keywords": [],
	"author": "Daniel Grossmann-Kavanagh",
	"license": "MIT",
	"devDependencies": {
		"@tsconfig/svelte": "^5.0.4",
		"@types/blessed": "^0.1.25",
		"@types/diff": "^5.0.9",
		"@types/diff-match-patch": "^1.0.36",
		"@types/eventsource": "^1.1.15",
		"@types/jest": "^29.5.12",
		"@types/node": "^16.11.6",
		"@types/path-browserify": "^1.0.2",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"builtin-modules": "3.3.0",
		"chokidar": "^3.6.0",
		"esbuild": "0.17.3",
		"esbuild-svelte": "^0.8.0",
		"jest": "^29.7.0",
		"obsidian": "latest",
		"svelte": "^4.2.19",
		"svelte-preprocess": "^5.1.4",
		"ts-jest": "^29.1.2",
		"tslib": "2.4.0",
		"typescript": "^5.4.5"
	},
	"dependencies": {
		"@y-sweet/client": "^0.1.0",
		"blessed": "^0.1.81",
		"crdt-debugger": "file:debugger",
		"diff": "^5.2.0",
		"diff-match-patch": "^1.0.5",
		"eventsource": "^2.0.2",
		"gray-matter": "^4.0.3",
		"jose": "^5.3.0",
		"lucide-svelte": "^0.377.0",
		"monkey-around": "^3.0.0",
